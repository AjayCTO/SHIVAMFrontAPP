                </article> <!-- card-group-item.// -->
                <article class="card" ng-repeat="Attribute in Attributes|limitTo:4" ng-init="outerIndex = $index">
                    <header class="card-header">
                        <a data-toggle="collapse" data-target="#collapse444_{{$index}}">
